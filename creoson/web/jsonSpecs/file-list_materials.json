{
  "spec" : {
    "function_description" : "List materials on a part",
    "command" : "file",
    "function" : "list_materials",
    "request" : [ {
      "name" : "file",
      "type" : "string",
      "description" : "Part name",
      "default" : "Currently active model"
    }, {
      "name" : "material",
      "type" : "string",
      "description" : "Material name pattern",
      "default" : "All materials",
      "wildcards_allowed" : true
    } ],
    "response" : [ {
      "name" : "materials",
      "type" : "array:string",
      "description" : "List of materials in the part"
    } ]
  },
  "examples" : [ {
    "request" : {
      "sessionId" : "~sessionId~",
      "command" : "file",
      "function" : "list_materials",
      "data" : {
        "file" : "box.prt"
      }
    },
    "response" : {
      "status" : {
        "error" : false
      },
      "data" : {
        "materials" : [ "brass", "bronze", "steel" ]
      }
    }
  }, {
    "request" : {
      "sessionId" : "~sessionId~",
      "command" : "file",
      "function" : "list_materials",
      "data" : {
        "file" : "box.prt",
        "material" : "br*"
      }
    },
    "response" : {
      "status" : {
        "error" : false
      },
      "data" : {
        "materials" : [ "brass", "bronze" ]
      }
    }
  }, {
    "request" : {
      "sessionId" : "~sessionId~",
      "command" : "file",
      "function" : "list_materials",
      "data" : {
        "file" : "box.prt",
        "material" : "brass"
      }
    },
    "response" : {
      "status" : {
        "error" : false
      },
      "data" : {
        "materials" : [ "brass" ]
      }
    }
  } ]
}